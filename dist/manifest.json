{
  "name": "Translation fix",
  "version": "1.0",
  "description": "修复翻译排版格式",
  "permissions": ["tabs", "activeTab", "storage", "<all_urls>", "webNavigation"],
  "options_page": "index.html#options",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "index.html#popup",
    "default_title": "这是一个示例Chrome插件",
    "default_icon": {
      "128": "assets/icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content.css"],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "128": "assets/icon.png"
  },
  "manifest_version": 2
}
