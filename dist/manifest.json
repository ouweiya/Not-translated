{
  "name": "Translation fix",
  "version": "1.0",
  "description": "修复翻译排版格式",
  "permissions": ["activeTab", "storage", "<all_urls>"],
  "options_page": "index.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "128": "assets/icon.png"
    }
  },
  "icons": {
    "128": "assets/icon.png"
  },
  "manifest_version": 2
}
